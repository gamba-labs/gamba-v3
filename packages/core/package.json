{
  "name": "@gamba/sdk",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./pdas": {
      "types": "./dist/pdas.d.ts",
      "import": "./dist/pdas.js",
      "require": "./dist/pdas.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./multiplayer": {
      "types": "./dist/multiplayer.d.ts",
      "import": "./dist/multiplayer.js",
      "require": "./dist/multiplayer.cjs"
    },
    "./referral": {
      "types": "./dist/referral.d.ts",
      "import": "./dist/referral.js",
      "require": "./dist/referral.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/core.ts src/multiplayer.ts src/referral.ts src/pdas.ts --dts --format esm,cjs --sourcemap",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts src/core.ts src/multiplayer.ts src/referral.ts src/pdas.ts --dts --format esm,cjs --sourcemap --watch"
  },
  "dependencies": {
    "@solana/kit": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
